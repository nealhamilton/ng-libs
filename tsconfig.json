{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-qr": [
        "dist/ngx-qr/ngx-qr",
        "dist/ngx-qr"
      ],
      "ngx-qr-reader": [
        "dist/ngx-qr-reader/ngx-qr-reader",
        "dist/ngx-qr-reader"
      ],
      "ngx-breadcrumbs": [
        "dist/ngx-breadcrumbs/ngx-breadcrumbs",
        "dist/ngx-breadcrumbs"
      ],
      "ngx-qrcode-scanner": [
        "dist/ngx-qrcode-scanner/ngx-qrcode-scanner",
        "dist/ngx-qrcode-scanner"
      ],
      "ngx-signature-pad": [
        "dist/ngx-signature-pad/ngx-signature-pad",
        "dist/ngx-signature-pad"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}